abstract = {To compare different scoring algorithms for Pick-N multiple correct answer multiple-choice (MC) exams regarding test reliability, student performance, total item discrimination and item difficulty. Data from six 3rd year medical students' end of term exams in internal medicine from 2005 to 2008 at Munich University were analysed (1,255 students, 180 Pick-N items in total). Scoring Algorithms: Each question scored a maximum of one point. We compared: (a) Dichotomous scoring (DS): One point if all true and no wrong answers were chosen. (b) Partial credit algorithm 1 (PS(50)): One point for 100\% true answers; 0.5 points for 50\% or more true answers; zero points for less than 50\% true answers. No point deduction for wrong choices. (c) Partial credit algorithm 2 (PS(1/m)): A fraction of one point depending on the total number of true answers was given for each correct answer identified. No point deduction for wrong choices. Application of partial crediting resulted in psychometric results superior to dichotomous scoring (DS). Algorithms examined resulted in similar psychometric data with PS(50) only slightly exceeding PS(1/m) in higher coefficients of reliability. The Pick-N MC format and its scoring using the PS(50) and PS(1/m) algorithms are suited for undergraduate medical examinations. Partial knowledge should be awarded in Pick-N MC exams.},
author = {Bauer, Daniel and Holzer, Matthias and Kopp, Veronika and Fischer, Martin R},
doi = {10.1007/s10459-010-9256-1},
file = {:C$\backslash$:/Users/maayan/Документы/My resources/Automatic test assessment/Pick-N multiple choicr-exams - a comparison of scoring algorithms.pdf:pdf},
issn = {1573-1677},
journal = {Advances in health sciences education : theory and practice},
keywords = {Algorithms,Attitudes,Education,Educational Measurement,Educational Measurement: methods,Educational Status,Germany,Health Knowledge,Humans,Internal Medicine,Internal Medicine: education,Internal Medicine: statistics \& numerical data,Medical,Nonparametric,Practice,Psychometrics,Schools,Statistics,Students,Task Performance and Analysis,Undergraduate,Undergraduate: methods},
month = may,
number = {2},
pages = {211--21},
pmid = {21038082},
title = {{Pick-N multiple choice-exams: a comparison of scoring algorithms.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21038082 http://www.springerlink.com/content/9u53270026883p26/fulltext.pdf},
volume = {16},
year = {2011}
}
@article{Frisbie1992,
abstract = {Reviews the literature related to the multiple true–false (MTF) item format to distinguish it from other related formats, to synthesize the research findings regarding its psychometric characteristics, and to propose a research agenda for learning more about the extent of its effectiveness in achievement testing. MTF was found to be a highly efficient format for gathering achievement data, tending to yield more reliable scores than multiple choice (MC) and other objective formats. MTF measured the same skills and abilities as content-parallel MC, was shown to be a bit harder than MC for examinees, and was perceived by examinees as harder but more efficient than MC. Improvements in the measurement of educational achievement can occur if MTF items are used more widely, whether for classroom assessment, program placement, or certification of competence. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
author = {Frisbie, David A.},
journal = {Educational Measurement: Issues and Practice},
pages = {21--26},
title = {{The multiple true-false item format: A status review}},
volume = {11(4)},
year = {1992}
}
